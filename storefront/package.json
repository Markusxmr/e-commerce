{
  "name": "storefront",
  "version": "0.0.1",
  "scripts": {
    "prepare": "pnpm run gen",
    "gen": "houdini generate -l full",
    "dev": "vite dev",
    "dev:css": "tailwindcss -w -i ./tailwind.css -o app/styles/app.css",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "pretty-quick"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@kitql/vite-plugin-watch-and-run": "^0.4.2",
    "@sveltejs/adapter-auto": "^1.0.0-next.87",
    "@sveltejs/adapter-node": "^1.0.0-next.100",
    "@sveltejs/adapter-vercel": "1.0.0-next.81",
    "@sveltejs/kit": "1.0.0-next.539",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "autoprefixer": "10.4.13",
    "daisyui": "2.38.1",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "houdini": "^0.17.5",
    "houdini-svelte": "^0.17.5",
    "husky": "8.0.2",
    "postcss": "8.4.18",
    "postcss-load-config": "4.0.1",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.8.0",
    "pretty-quick": "3.1.3",
    "svelte": "3.52.0",
    "svelte-check": "2.9.2",
    "svelte-preprocess": "4.10.7",
    "tailwindcss": "3.2.2",
    "tslib": "2.4.1",
    "typescript": "4.8.4",
    "vite": "3.2.3"
  },
  "type": "module",
  "dependencies": {
    "@kitql/all-in": "0.7.10",
    "@stripe/stripe-js": "1.42.1",
    "@tailwindcss/forms": "0.5.3",
    "graphql": "16.6.0",
    "svelte-stripe": "0.0.19"
  }
}